@{
}

@model CMSAmenityCreateViewModel



@{
    foreach(var item in Model.Amenity)
    {
        <form method="post" asp-controller="Amenity" asp-action="Delete">
        <p>@item.Name</p>
        <input type="hidden" value="@item.Id" name="id">
        <input type="submit" value="Delete">
        </form>
    }
}