@{
}

@model CMSRoomViewModel

@{
    foreach(var item in Model.RoomNumbers)
    {
        <form method="post" asp-controller="Room" asp-action="Delete">
        <p>Room Number: @item</p>
        <input type="hidden" value="@item" name="roomNumber">
        <input type="submit" value="Delete">
        </form>
    }
}