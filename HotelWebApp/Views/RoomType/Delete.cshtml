@{
}

@model CMSRoomTypeCreateViewModel

@{
    foreach(var item in Model.RoomTypes)
    {
        <form method="post" asp-controller="RoomType" asp-action="Delete">
        <p>@item.Title</p>
        <input type="hidden" value="@item.Id" name="id">
        <input type="submit" value="Delete">
        </form>
    }
}